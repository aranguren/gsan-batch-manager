<div class="table-responsive">
  <div class="panel panel-default">
    <div class="panel-heading">Processos</div>
    <center>
      <%= paginate processos %>
    </center>
    <table class="table table-striped">
      <thead>
        <tr>
          <th>#</th>
          <th>Processo</th>
          <th>Grupo</th>
          <th>Início</th>
          <th>Término</th>
          <th>Usuário</th>
        </tr>
      </thead>
      <tbody>
      <% processos.each do |processo| %>
        <tr>
          <td><%= processo.id %></td>
          <td><%= processo.nome %></td>
          <td><%= processo.grupo unless processo.grupo.blank? %></td>
          <td><%= processo.hora_inicio.strftime('%d/%m/%Y %H:%M:%S') unless processo.hora_inicio.blank? %></td>
          <td><%= processo.hora_termino.strftime('%d/%m/%Y %H:%M:%S') unless processo.hora_termino.blank? %></td>
          <td><%= processo.usuario.nome %></td>
        </tr>
      <% end %>
      </tbody>
    </table>
    <center>
      <%= paginate processos %>
    </center>
  </div>
</div>
